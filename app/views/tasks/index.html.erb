<h1>Index of tasks</h1>
<%= link_to "Add a task", new_task_path %>
<% @tasks.each do |task| %>
  <h2> <%= link_to task.content, task_path(task)%></h2>
  <p> <%= task.done%></p>
  <p> <%= task.priority %></p>
  <%= link_to "Edit", edit_task_path(task) %>
  <%= link_to "Delete", task_path(task), method: :delete %>
  <hr>
  <%end %>


